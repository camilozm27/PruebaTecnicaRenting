{
  "PathBase": "/pruebatecnicarenting",
  "HealthCheck": {
    "Database": {
      "Timeout": 30000
    },
    "RoutePrefix": "/api/help"
  },
  "ConnectionStrings": {
    "Base": "Server=localhost;Database=PruebaRenting;Trusted_Connection=SSPI;MultipleActiveResultSets=true;Trust Server Certificate=true",
    "BaseSchema": "dbo"
  },
  "RetryPolicy": {
    "RetryCount": "3",
    "RetrySeconds": "2"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowSpecificOrigins": {
    "domains": [
      "http://localhost:4200"
    ]
  }
}
